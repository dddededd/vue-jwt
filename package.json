    "js-base64": "^3.7.2",
    "material-design-icons-iconfont": "6.1.0",
    "moment": "^2.29.1",
    "roboto-fontface": "*",
    "vue": "^2.6.11",
    "vue-i18n": "8",
    "vue-head": "^2.2.0",
    "vue-router": "^3.5.3",
    "vuetify": "^2.4.0",
    "vuex": "^3.4.0"
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{scss,vue}": "stylelint"
